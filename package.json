{
  "name": "@malereg/aowatch-client",
  "version": "1.0.16",
  "description": "Abgeordnetenwatch TypeScript/JavaScript API client for Browser, Node and Typescript",
  "main": "./dist-node/aowatch-client.js",
  "exports": {
    ".": "./dist-node/aowatch-client.js"
  },
  "keywords": [
    "bundestag",
    "opendata",
    "abgeordnetenwatch",
    "politics",
    "api",
    "client"
  ],
  "scripts": {
    "test": "./node_modules/ts-mocha/bin/ts-mocha -p ./tsconfig.json ./__test__/*.test.ts",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "build:node": "tsc --p ./tsconfig.node.json",
    "clean": "shx rm -rf dist-web dist dist-node",
    "build:web": "npm run clean && webpack && cp ./dist/aowatch-client-bundle.js ./dist-web/aowatch-client-bundle.js",
    "build:docs": "typedoc src/* --includeVersion"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maschinenlesbareregierung/aowatch-client.git"
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.24",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/qs": "^6.9.5",
    "auto-changelog": "^2.2.1",
    "chai": "^4.2.0",
    "microbundle": "^0.13.0",
    "mocha": "^8.2.1",
    "nock": "^13.0.5",
    "shx": "^0.3.3",
    "ts-loader": "^8.0.13",
    "ts-mocha": "^8.0.0",
    "typed-emitter": "^1.3.1",
    "typedoc": "^0.20.13",
    "typescript": "^4.1.3",
    "webpack": "^4.4.2",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@types/node": "^14.14.12",
    "axios": "^0.21.0",
    "qs": "^6.9.4"
  }
}
